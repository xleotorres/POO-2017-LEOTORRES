package model;

public class Funcionario {

	public String nome;
	public double salario;
	public String departamento;
	public String data;
	public String RG;
	
	void recebeAumento (double aumento) {
		salario = salario + aumento;
	}
	
	void calculaGanhoanual() {
		double ganho_anual;
		ganho_anual = salario * 12;
	}
	
	
}
